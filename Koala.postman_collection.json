									"listen": "test",
									"script": {
										"exec": [
											"pm.test(\"Login successful\", function () {",
											"    const response = pm.response.json();",
											"    ",
											"    if (response.code === 2000) {",
											"        console.log(`Login with ${response.data.profile.username} successfully`);",
											"        ",
											"        // 設置 access token 和 refresh token",
											"        pm.environment.set(\"member_access_token\", response.data.tokens.access_token);",
											"        pm.environment.set(\"member_refresh_token\", response.data.tokens.refresh_token);",
											"        ",
											"        console.log(\"Tokens saved to environment variables\");",
											"    } else {",
											"        console.error(`Login failed. Response code:`, response.code);",
											"    }",
											"});"
										],
