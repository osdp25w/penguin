# Map provider (固定 maplibre)
VITE_MAP_PROVIDER=maplibre

# NLSC WMTS layer（可選：EMAP | EMAP2 | PHOTO2）
VITE_EMAP_LAYER=EMAP
VITE_EMAP_MATRIXSET=GoogleMapsCompatible

# Mock Data Settings
VITE_SEED_MOCK=0
VITE_SEED_REGION=hualien

# Map Configuration
VITE_MAP_CENTER=23.8,121.6
VITE_MAP_ZOOM=10

# Koala API base (default to https://koala.osdp25w.xyz if not set)
VITE_KOALA_BASE_URL=https://koala.osdp25w.xyz

# Koala encryption keys
# Use this to encrypt plaintext password on login (Fernet base64url key)
VITE_KOALA_LOGIN_KEY=
# Use this to decrypt sensitive info from GET APIs (reserved for future use)
VITE_KOALA_SENSITIVE_KEY=

# Dev only: force deterministic Fernet using fixed timestamp and IV (16-byte hex)
# Example to reproduce a known token:
# VITE_KOALA_FORCE_FERNET_TS=1756649913
# VITE_KOALA_FORCE_FERNET_IV=6d70c048bb75828834805cdd5b0616e8
# If backend swapped key halves (rare), enable compat
# VITE_KOALA_FORCE_FERNET_COMPAT=1
